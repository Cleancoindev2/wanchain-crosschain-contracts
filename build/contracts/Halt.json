  "source": "pragma solidity ^0.4.24;\nimport './Owned.sol';\n\ncontract Halt is Owned {\n    \n    bool public halted = true; \n    \n    modifier notHalted() {\n        require(!halted);\n        _;\n    }\n\n    modifier isHalted() {\n        require(halted);\n        _;\n    }\n    \n    /// @notice function Emergency situation that requires \n    /// @notice contribution period to stop or not.\n    function setHalt(bool halt) \n        public \n        onlyOwner\n    {\n        halted = halt;\n    }\n}",
